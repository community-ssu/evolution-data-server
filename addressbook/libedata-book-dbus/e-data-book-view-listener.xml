<node>

  <interface name="org.gnome.evolution.dataserver.addressbook.DataBookViewListener">
    <annotation name="org.freedesktop.DBus.GLib.CSymbol" value="EDataBookViewListener"/>

    <method name="notifyContactsAdded">
      <annotation name="org.freedesktop.DBus.Method.NoReply" value="yes"/>
      <annotation name="org.freedesktop.DBus.GLib.CSymbol" value="impl_notifyContactsAdded"/>
      <arg name="vcards" type="as" direction="in"/>
    </method>

    <method name="notifyContactsChanged">
      <annotation name="org.freedesktop.DBus.Method.NoReply" value="yes"/>
      <annotation name="org.freedesktop.DBus.GLib.CSymbol" value="impl_notifyContactsChanged"/>
      <arg name="vcards" type="as" direction="in"/>
    </method>

    <method name="notifyContactsRemoved">
      <annotation name="org.freedesktop.DBus.Method.NoReply" value="yes"/>
      <annotation name="org.freedesktop.DBus.GLib.CSymbol" value="impl_notifyContactsRemoved"/>
      <arg name="ids" type="as" direction="in"/>
    </method>

    <method name="notifyStatusMessage">
      <annotation name="org.freedesktop.DBus.Method.NoReply" value="yes"/>
      <annotation name="org.freedesktop.DBus.GLib.CSymbol" value="impl_notifyStatusMessage"/>
      <arg name="message" type="s" direction="in"/>
    </method>

    <method name="notifyComplete">
      <annotation name="org.freedesktop.DBus.Method.NoReply" value="yes"/>
      <annotation name="org.freedesktop.DBus.GLib.CSymbol" value="impl_notifyComplete"/>
      <arg name="status" type="u" direction="in"/>
    </method>

  </interface>

</node>
